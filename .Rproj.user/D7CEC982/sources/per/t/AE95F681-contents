if (numero > 0) {
positivos <- c(positivos, numero)
} else if (numero < 0) {
negativos <- c(negativos, numero)
}
}
cat("Números positivos:", positivos, "\n")
cat("Números negativos:", negativos, "\n")
set.seed(123)
numeros <- sample(1:1000, 100, replace = TRUE)
cat("Número\t\tClasificación\n")
cat("----------------------------\n")
for (numero in numeros) {
if (numero %% 2 == 0) {
clasificacion <- "Par"
} else {
clasificacion <- "Impar"
}
cat(numero, "\t\t", clasificacion, "\n")
}
